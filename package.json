{
  "name": "pong3",
  "version": "1.0.0",
  "description": "Solana-based Pong game with AI opponent",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy": "aws elasticbeanstalk create-application-version --application-name 0xPong3 --version-label $(date +%Y%m%d-%H%M%S) --source-bundle S3Bucket=elasticbeanstalk-us-east-1-YOUR_ACCOUNT_ID,S3Key=0xPong3.zip"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "@solana/web3.js": "^1.78.4",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "aws-sdk": "^2.1467.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
